{
	"name": "astro-cactus",
	"version": "2.0.2",
	"private": false,
	"type": "module",
	"engines": {
		"node": ">=20.6.1",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"setup": "npx vite-node --config setup.vite.config.ts setup.ts",
		"dev": "astro dev",
		"start": "astro dev",
		"sync": "astro sync",
		"build": "prisma generate && astro build",
		"preview": "astro preview",
		"format": "prettier -w ./src ./src/**/*.astro",
		"validate:recipes": "node -e \"(async()=>{const {readdirSync,readFileSync}=await import('fs');const Cook=await import('cooklang');let ok=true;for(const f of readdirSync('src/recipes')){if(!f.endsWith('.cook')) continue;const raw=readFileSync('src/recipes/'+f,'utf8');try{(Cook as any).parse? (Cook as any).parse(raw) : (Cook as any)(raw);}catch(e){console.error('Invalid recipe:',f);console.error(e.message||e);ok=false;}}if(!ok) process.exit(1);else console.log('All recipes valid');})()\""
	},
